    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 324
        },
        "id": "OGC2Vpg-xg2k",
        "outputId": "19bc75a1-0b8f-4b7b-bc35-3350abe5eb89"
